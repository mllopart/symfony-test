{% extends 'base.html.twig' %}

{% block title %}{{ parent() }}Best Cars Offers{% endblock %}

{% block body %}
    <h1>Offer</h1>

    <table class="table">
      <tr>
        <th>Make</th>
        <th>Name</th>
      </tr>
      {% for car in cars %}
        <tr>
          <td>{{ car.make }}</td>
          <td>{{ car.name }}</td>
        </tr>
      {% endfor %}
    </table>

{% endblock %}
